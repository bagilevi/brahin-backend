<!DOCTYPE html>
<html>
  <head>
    <title>Memonite</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_tag "var authenticityToken = '#{form_authenticity_token}';" if protect_against_forgery? %>
    <%= javascript_include_tag 'jquery-3.3.1.min.js' %>
    <%= javascript_include_tag 'init' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
